{
  "name": "re-queuejs",
  "version": "4.0.0",
  "description": "A lightweight nodejs and redis queue manager",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "files": [
    "src/**/*",
    "dist/**/*",
    "*.d.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndekesyiri/requeue.git"
  },
  "keywords": [
    "Redis",
    "queue",
    "manager",
    "memory",
    "cache",
    "hooks",
    "performance"
  ],
  "dependencies": {
    "ioredis": "^5.3.2",
    "lru-cache": "^10.0.0",
    "uuid": "^9.0.0"
  },
  "author": "ndekesyiri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndekesyiri/requeue/issues"
  },
  "homepage": "https://github.com/ndekesyiri/requeue#readme",
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/node": "^24.6.0",
    "jest": "^30.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
